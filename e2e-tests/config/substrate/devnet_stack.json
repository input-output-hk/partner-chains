{
    "stack_config": {
        "ogmios_scheme": "ws",
        "ogmios_host": "devnet-services-service",
        "ogmios_port": 1337,
        "tools": {
            "cardano_cli": {
                "cli": "export CARDANO_NODE_SOCKET_PATH=/ipc/node.socket && /tools/cardano-cli",
                "pod": "devnet-services",
                "namespace": "sc",
                "container": "cardano-node"
            },
            "partner_chains_node": {
                "cli": "/partner-chains-node",
                "pod": "alice",
                "namespace": "sc",
                "container": "substrate-node"
            },
            "bech32": {
                "cli": "/tools/bech32",
                "ssh": "${stack_config[ssh]}"
            }
        }
    }
}
